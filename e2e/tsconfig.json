{
  "compilerOptions": {
    "target": "es2020",
    "lib": [ "dom", "dom.iterable", "esnext"],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["cypress", "node"]
  },
  "include": ["cypress/**/*.ts"]
}
